{
  "name": "vue-testing-demo",
  "version": "1.0.0",
  "description": "Simple Vue app for testing npm packages",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite --port 3001",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:run": "vitest run",
    "vitest:wax": "vitest run src/tests/vitest/app.wax.test.js",
    "jest:test": "jest",
    "jest:wax": "jest src/tests/jest/app.wax.test.js",
    "mocha:test": "mocha",
    "mocha:wax": "mocha src/tests/mocha/app.wax.test.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:wax": "cypress run --spec 'cypress/e2e/app.wax.cy.js'",
    "playwright:install": "playwright install",
    "playwright:test": "playwright test",
    "playwright:test:ui": "playwright test --ui",
    "playwright:wax": "playwright test app.wax.spec.js",
    "webdriverio:test": "wdio run wdio.conf.cjs",
    "webdriverio:wax": "wdio run wdio.conf.cjs --spec src/tests/webdriverio/app.wax.spec.cjs",
    "testcafe:test": "testcafe chrome src/tests/testcafe/",
    "testcafe:wax": "testcafe chrome src/tests/testcafe/app.wax.test.js",
    "nightwatch:test": "nightwatch",
    "nightwatch:wax": "nightwatch --test src/tests/nightwatch/app.wax.test.js",
    "e2e:all": "npm run cypress:run && npm run playwright:test && npm run webdriverio:test && npm run testcafe:test && npm run nightwatch:test",
    "wax:all": "npm run vitest:wax && npm run mocha:wax && npm run cypress:wax && npm run playwright:wax && npm run webdriverio:wax && npm run testcafe:wax && npm run nightwatch:wax"
  },
  "dependencies": {
    "@wally-ax/wax-dev": "^0.2.62",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "@wdio/cli": "^9.19.1",
    "@wdio/local-runner": "^9.19.1",
    "@wdio/mocha-framework": "^9.19.1",
    "@wdio/spec-reporter": "^9.19.1",
    "babel-jest": "^30.0.5",
    "chai": "^4.3.7",
    "cypress": "^14.5.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^23.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^11.7.1",
    "nightwatch": "^3.0.0",
    "node-fetch": "^2.7.0",
    "testcafe": "^3.6.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "webdriverio": "^8.0.0"
  }
}
