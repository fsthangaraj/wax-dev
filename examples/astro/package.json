{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:wax": "cypress run --spec 'cypress/e2e/app.wax.cy.js'",
    "playwright:install": "playwright install",
    "playwright:test": "playwright test",
    "playwright:test:ui": "playwright test --ui",
    "playwright:wax": "playwright test tests/playwright/app.wax.spec.cjs",
    "webdriverio:test": "wdio run wdio.conf.cjs",
    "webdriverio:wax": "wdio run wdio.conf.cjs --spec tests/webdriverio/app.wax.spec.cjs",
    "testcafe:test": "testcafe chrome tests/testcafe/",
    "testcafe:wax": "testcafe chrome tests/testcafe/app.wax.test.js",
    "nightwatch:test": "nightwatch",
    "nightwatch:wax": "nightwatch --test tests/nightwatch/app.wax.test.cjs",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:run": "vitest run",
    "vitest:wax": "vitest run tests/vitest/app.wax.test.js",
    "jest:test": "jest",
    "jest:wax": "jest tests/jest/app.wax.test.cjs",
    "mocha:test": "mocha",
    "mocha:wax": "mocha tests/mocha/app.wax.test.cjs",
    "e2e:all": "npm run cypress:run && npm run playwright:test && npm run webdriverio:test && npm run testcafe:test && npm run nightwatch:test",
    "wax:all": "npm run vitest:wax && npm run jest:wax && npm run mocha:wax && npm run cypress:wax && npm run playwright:wax && npm run webdriverio:wax && npm run testcafe:wax && npm run nightwatch:wax"
  },
  "dependencies": {
    "astro": "^5.13.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@wally-ax/wax-dev": "^0.2.62",
    "@wdio/cli": "^9.19.1",
    "@wdio/local-runner": "^9.19.1",
    "@wdio/mocha-framework": "^9.19.1",
    "@wdio/spec-reporter": "^9.19.1",
    "chai": "^4.3.7",
    "chromedriver": "^139.0.2",
    "cypress": "^15.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^11.7.1",
    "nightwatch": "^3.12.2",
    "node-fetch": "^2.7.0",
    "playwright": "^1.55.0",
    "testcafe": "^3.7.2",
    "vitest": "^3.2.4",
    "wdio-chromedriver-service": "^8.1.1",
    "webdriverio": "^9.19.1"
  }
}
